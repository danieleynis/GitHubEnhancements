{
  "manifest_version": 2,
  "name": "GitHub PR Links",
  "version": "0.1",
  "permissions": ["tabs"],
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "js": ["jquery-3.3.1.min.js", "content.js"]
    }
  ]
}